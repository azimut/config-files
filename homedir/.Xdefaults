! █▓▒░ fonts
urxvt*font: xft:gohufont:size=11:antialias=true
urxvt*boldFont: xft:gohufont:size=11:antialias=true
urxvt*lineSpace: -1

! █▓▒░ interface
urxvt*scrollBar: false
urxvt*transparent: true
urxvt*shading: 10

! █▓▒░ mikado colors
