xmobar.depth: 32
xmobar*background: rgba:0000/0000/0000/cccc
 
URxvt*termName: rxvt

!URxvt.depth: 32
!URxvt.background: rgba:1111/1111/1111/dddd

!URxvt*depth: 32
!URxvt*background: rgba:0000/0000/0200/c800

! borderless and no scrollbar
URxvt*scrollBar_right: false
URxvt*scrollBar: false
URxvt*borderLess: false
 
! teh transparency stuff
URxvt*inheritPixmap: true
URxvt*tintColor: white
URxvt*shading: 15
 
! geometry and font
URxvt*geometry: 84Ã—24
!URxvt*font:               xft:Terminus:antialias=false:pixelsize=9:12
!urxvt*font:       xft:Terminus:antialias=false:7
!URxvt*font: xft:Terminus:pixelsize=12,xft:Inconsolata\ for\ Powerline:pixelsize=12
!URxvt.font: xft:Source Code Pro Medium for Powerline
!URxvt*font: xft:terminus:antialias=false:pixelsize=12
!URxvt*font: xft:Source Code Pro for Powerline,Source Code Pro Medium
!URxvt.font: xft:Source Code Pro Medium for Powerline:pixelsize=10:antialias=true:hinting=true
!URxvt*font: xft:Source Code Pro Medium,xft:Source Code Pro Medium for Powerline

!urxvt*cursorColor: #DCDCCC
urxvt*cursorBlink:       true
urxvt*urgentOnBell:       true

